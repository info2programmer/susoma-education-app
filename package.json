      "cordova-plugin-youtube-video-player": {},
      "cordova-clipboard": {},
      "cordova-plugin-sms-receive": {},
      "cordova-plugin-chooser": {},
      "cordova-plugin-spinner": {},
      "cordova-plugin-appversion": {},
      "cordova-plugin-fcm": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "dependencies": {
    "com.telerik.plugins.nativepagetransitions": "~0.6.5",
    "cordova": "^9.0.0",
    "cordova-android": "^8.0.0",
