{
  "name": "framework7-template-split-view",
  "version": "1.0.0",
  "description": "Framework7 split view starter app template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framework7io/framework7-template-split-view.git"
  },
  "scripts": {
    "serve": "http-server ./www/ -o -c 1 -a localhost",
    "copy-f7": "cpy node_modules/framework7/js/*.* www/framework7/js && cpy node_modules/framework7/css/*.* www/framework7/css && cpy node_modules/framework7-icons/fonts/*.* www/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} www/fonts",
    "postinstall": "npm run copy-f7"
  },
  "keywords": [
    "framework7",
    "template",
    "ios",
    "android",
    "mobile",
    "app",
    "cordova",
    "phonegap"
  ],
  "author": "Vladimir Kharlampidi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framework7io/framework7-template-split-view/issues"
  },
  "homepage": "https://github.com/framework7io/framework7-template-split-view#readme",
  "devDependencies": {
    "cpy-cli": "^1.0.1",
    "framework7": "^3.0.0",
    "framework7-icons": "^0.9.1",
    "http-server": "^0.10.0",
    "material-design-icons": "^3.0.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-battery-status": {},
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "GURUSIKSHA app reqiires access to the CAMERA",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "GURUSIKSHA app reqiires access to the PHOTOLIBRARY."
      },
      "cordova-plugin-media-capture": {},
      "cordova-plugin-device": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-file": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-globalization": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-x-toast": {},
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-device-motion": {},
      "cordova-plugin-device-orientation": {},
      "com.telerik.plugins.nativepagetransitions": {},
      "cordova-plugin-console": {},
      "cordova-plugin-contacts": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "susoma"
      },
      "cordova-plugin-code-push": {},
      "cordova-plugin-file-transfer": {},
      "cordova-universal-links-plugin": {},
      "cordova-android-support-gradle-release": {
        "ANDROID_SUPPORT_VERSION": "27.+"
